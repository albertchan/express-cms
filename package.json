{
  "name": "express-cms",
  "description": "A content management system built on Express",
  "author": "Albert Chan",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albertchan/express-cms.git"
  },
  "keywords": [
    "Bookshelf.js",
    "Brunch",
    "Express",
    "Node.js"
  ],
  "scripts": {
    "dev": "nodemon -w src --ignore src/client/ --exec \"babel-node src/server --presets es2015,stage-0\"",
    "start": "brunch watch --server",
    "prod": "brunch build --production"
  },
  "dependencies": {
    "bluebird": "~3.4.0",
    "body-parser": "~1.15.0",
    "bookshelf": "~0.10.0",
    "compression": "~1.6.0",
    "cors": "~2.8.0",
    "express": "~4.13.0",
    "hbs": "~4.0.0",
    "knex": "~0.11.0",
    "lodash": "~4.15.0",
    "oauth2orize": "~1.5.0",
    "pg": "~4.1.0",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2.0.0",
    "babel-brunch": "~6.0.0",
    "babel-cli": "~6.14.0",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-stage-0": "~6.5.0",
    "brunch": "^2.4.0",
    "clean-css-brunch": "^2.0.0",
    "css-brunch": "^2.0.0",
    "debug": "~2.2.0",
    "javascript-brunch": "^2.0.0",
    "uglify-js-brunch": "^2.0.0"
  }
