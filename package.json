{
  "name": "express-cms",
  "description": "A content management system built on Express",
  "author": "Albert Chan",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albertchan/express-cms.git"
  },
  "keywords": [
    "Bookshelf.js",
    "Brunch",
    "Express",
    "Node.js"
  ],
  "scripts": {
    "build-server": "./node_modules/.bin/babel ./src/index.js --out-file prod-server.js",
    "dev": "concurrently --kill-others \"npm run dev-client\" \"npm run dev-server\"",
    "dev-client": "brunch watch --server",
    "dev-server": "nodemon -w src --ignore src/client/ --exec \"babel-node src/server --presets es2015,stage-0\"",
    "prod": "brunch build --production",
    "prod-server": "node ./prod-server.js"
  },
  "dependencies": {
    "bluebird": "~3.4.0",
    "body-parser": "~1.15.0",
    "bookshelf": "~0.10.0",
    "compression": "~1.6.0",
    "cors": "~2.8.0",
    "express": "~4.13.0",
    "hbs": "~4.0.0",
    "knex": "~0.11.0",
    "lodash": "~4.15.0",
    "node-uuid": "~1.4.0",
    "oauth2orize": "~1.5.0",
    "pg": "~4.1.0",
    "validator": "~5.7.0",
    "vue": "~2.0.0-rc.6",
    "vue-server-renderer": "~2.0.0-rc.6",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "babel-brunch": "~6.0.0",
    "babel-cli": "~6.14.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-stage-0": "~6.5.0",
    "bootstrap": "~3.3.0",
    "brunch": "^2.4.0",
    "clean-css-brunch": "^2.0.0",
    "concurrently": "~2.2.0",
    "css-brunch": "^2.0.0",
    "debug": "~2.2.0",
    "javascript-brunch": "^2.0.0",
    "sass-brunch": "^2.6.0",
    "uglify-js-brunch": "^2.0.0"
  }
}
